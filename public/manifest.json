{
    "manifest_version": 3,
    "name": "chrome-ext",
    "version": "1.0.0",
    "description": "chrome extension",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}